@{
    ViewData["Title"] = "Search Results";
    var genres = ViewBag.Genres as List<string> ?? new List<string>(); // Ép kiểu genres từ ViewBag
}
<div class="c-search-header__wrapper">
    <div class="container">
        <div class="search-content">
            <form method="get" id="search-page-form" action="/Search/Index">
                <span class="screen-reader-text">Tìm kiếm:</span>
                <input type="text" class="search-field manga-search-field" placeholder="Tìm Kiếm..." value="@ViewBag.SearchQuery" name="searchQuery">
                <input type="hidden" name="genre" value="@ViewBag.Genre">
                <input type="hidden" name="status" value="@ViewBag.Status">
                <input type="hidden" name="page" value="@ViewBag.CurrentPage">
                <input type="submit" class="search-submit" value="Tìm Kiếm">
            </form>
        </div>
        <div class="search-filters">
            <!-- Bộ lọc Genre -->
            <div class="genres-filter">
                <p class="filter-title">Thể Loại</p>
                <div class="options">
                    @foreach (var genre in genres.Take(3))
                    {
                        <a href="/Search/Index?searchQuery=@ViewBag.SearchQuery&genre=@genre&sort=@ViewBag.Sort" class="btn btn-outline-secondary @(ViewBag.Genre == genre ? "active" : "")">@genre</a>
                    }
                    @if (genres.Count > 3)
                    {
                        <div class="dropdown more-genres-dropdown">
                            <button class="btn dropdown-toggle" type="button" data-bs-toggle="dropdown" aria-expanded="false">
                                Xem Thêm
                            </button>
                            <div class="dropdown-menu">
                                @foreach (var genre in genres.Skip(3))
                                {
                                    <a href="/Search/Index?searchQuery=@ViewBag.SearchQuery&genre=@genre&sort=@ViewBag.Sort" class="dropdown-item btn btn-outline-secondary @(ViewBag.Genre == genre ? "active" : "")">@genre</a>
                                }
                            </div>
                        </div>
                    }
                </div>
            </div>
            <span class="vertical divider"></span>
            <!-- Bộ lọc Status (dùng tiếng Việt) -->
            <div class="filter-status">
                <p class="filter-title">Trạng Thái</p>
                <div class="dropdown options">
                    <button class="btn dropdown-toggle" type="button" data-bs-toggle="dropdown" aria-expanded="false">
                        @(string.IsNullOrEmpty(ViewBag.Status) ? "Trạng Thái" : ViewBag.Status)
                    </button>
                    <div class="dropdown-menu">
                        <a href="/Search/Index?searchQuery=@ViewBag.SearchQuery&genre=@ViewBag.Genre&status=đang tiến hành&sort=@ViewBag.Sort" class="dropdown-item btn btn-outline-secondary @(ViewBag.Status == "đang tiến hành" ? "active" : "")">Đang tiến hành</a>
                        <a href="/Search/Index?searchQuery=@ViewBag.SearchQuery&genre=@ViewBag.Genre&status=hoàn thành&sort=@ViewBag.Sort" class="dropdown-item btn btn-outline-secondary @(ViewBag.Status == "hoàn thành" ? "active" : "")">Hoàn thành</a>
                    </div>
                </div>
            </div>
            <span class="vertical divider"></span>
            <!-- Bộ lọc Sort -->
            <div class="filter-sort">
                <p class="filter-title">Sắp Xếp</p>
                <div class="dropdown options">
                    <button class="btn dropdown-toggle" type="button" data-bs-toggle="dropdown" aria-expanded="false">
                        @(ViewBag.Sort switch
                        {
                            "a-z" => "A-Z",
                            "z-a" => "Z-A",
                            _ => "Mặc định"
                        })
                    </button>
                    <div class="dropdown-menu">
                        <a href="/Search/Index?searchQuery=@ViewBag.SearchQuery&genre=@ViewBag.Genre&status=@ViewBag.Status&sort=default" class="dropdown-item btn btn-outline-secondary @(ViewBag.Sort == "default" ? "active" : "")">Mặc định</a>
                        <a href="/Search/Index?searchQuery=@ViewBag.SearchQuery&genre=@ViewBag.Genre&status=@ViewBag.Status&sort=a-z" class="dropdown-item btn btn-outline-secondary @(ViewBag.Sort == "a-z" ? "active" : "")">A-Z</a>
                        <a href="/Search/Index?searchQuery=@ViewBag.SearchQuery&genre=@ViewBag.Genre&status=@ViewBag.Status&sort=z-a" class="dropdown-item btn btn-outline-secondary @(ViewBag.Sort == "z-a" ? "active" : "")">Z-A</a>
                    </div>
                </div>
            </div>
            <span class="vertical divider"></span>
            <!-- Nút Reset Filter -->
            <div class="reset-filter">
                <a href="/Search/Index" class="btn btn-outline-danger"><i class="fas fa-times"></i> Đặt lại bộ lọc</a>
            </div>
        </div>
    </div>
</div>

<div class="c-page-content">
    <div class="content-area">
        <div class="container">
            <div class="row">
                <div class="main-col col-md-12 sidebar-hidden">
                    <div class="main-col-inner">
                        <div class="search-wrap">
                            <h3 class="h4">
                                <i class="icon ion-ios-star"></i> @ViewBag.TotalItems kết quả được tìm thấy 
                            </h3>
                            <div class="search-lists">
                                @foreach (var comic in Model)
                                {
                                    <div class="manga__item">
                                        <div class="manga__thumb">
                                            <div class="manga__thumb_item">
                                                <a href="/Comic/Detail/@comic.Id">
                                                    <img width="110" height="150" src="@comic.CoverImage" class="attachment-manga-thumb-1 size-manga-thumb-1 wp-post-image" alt="@comic.Title">
                                                </a>
                                            </div>
                                        </div>
                                        <div class="manga__content">
                                            <div class="manga__content_item">
                                                <div class="post-title font-title">
                                                    <h2>
                                                        <a href="/Comic/Detail/@comic.Id">@comic.Title</a>
                                                    </h2>
                                                    <div class="manga-excerpt">
                                                        <p>@comic.Description</p>
                                                    </div>
                                                    <div class="manga-info">
                                                        <span class="total">@comic.ViewCount View</span>
                                                    </div>
                                                    <span class="manga-genres">
                                                        @foreach (var genre in comic.Genres)
                                                        {
                                                            <span class="genre-item">
                                                                <i class="fas fa-circle"></i>
                                                                <a href="/Search/Index?searchQuery=@ViewBag.SearchQuery&genre=@genre">@genre</a>
                                                            </span>
                                                        }
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                }
                            </div>

                            <!-- Phân trang -->
                            <nav class="navigation paging-navigation">
                                <div class="col-12 col-md-12">
                                    <h4 class="screen-reader-text">Posts navigation</h4>
                                    <div class="nav-links">
                                        @if (ViewBag.CurrentPage > 1)
                                        {
                                            <div class=" float-left">
                                                <a href="/Search/Index?searchQuery=@ViewBag.SearchQuery&genre=@ViewBag.Genre&status=@ViewBag.Status&page=@(ViewBag.CurrentPage - 1)">
                                                    <span class="icon ion ion-md-arrow-back"></span> Trước
                                                </a>
                                            </div>
                                        }
                                        @if (ViewBag.CurrentPage < ViewBag.TotalPages)
                                        {
                                            <div class=" float-right">
                                                <a href="/Search/Index?searchQuery=@ViewBag.SearchQuery&genre=@ViewBag.Genre&status=@ViewBag.Status&page=@(ViewBag.CurrentPage + 1)">
                                                    Sau <span class="icon ion ion-md-arrow-forward"></span>
                                                </a>
                                            </div>
                                        }
                                    </div>
                                </div>
                            </nav>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

